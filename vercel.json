{
  "crons": [
    {
      "path": "/api/cron/process-scheduled-emails",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/process-reengage-emails",
      "schedule": "0 10 * * *"
    }
  ],
  "functions": {
    "app/api/account/deletion-request/route.js": {
      "maxDuration": 30
    },
    "app/api/cron/process-scheduled-emails/route.js": {
      "maxDuration": 60
    },
    "app/api/cron/process-reengage-emails/route.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/auth/callback",
      "destination": "/api/auth/callback"
    }
  ]
}
